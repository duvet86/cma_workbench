{
  "extends": "react-app",
  "rules": {
    "no-var": "error",
    "semi": ["error", "always"],
    "eol-last": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "jsx-a11y/alt-text": [0],
    "react/jsx-no-bind": [
      2,
      {
        "ignoreRefs": true
      }
    ]
  }
}
